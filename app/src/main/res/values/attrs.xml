<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomAppBar">
        <attr name="app_bar_icon_color" format="color" />
        <attr name="app_bar_text_color" format="color" />
        <attr name="app_bar_title" format="string" />
        <attr name="app_bar_bg_color" format="color" />
        <attr name="app_has_back_button" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleView">
        <attr name="cv_color" format="color" />
        <attr name="cv_border" format="boolean" />
        <attr name="cv_border_width" format="dimension" />
        <attr name="cv_border_color" format="color" />
        <attr name="cv_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="DotIndicator">
        <attr name="dotsSelectedColor" format="color" />
        <attr name="dotsColor" format="color" />
        <attr name="dotsSize" format="dimension" />
        <attr name="dotsCornerRadius" format="dimension" />
        <attr name="dotsSpacing" format="dimension" />
    </declare-styleable>
</resources>