<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavigationView.Active" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">@dimen/et_text_size_12</item>
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/color_FFB340</item>
    </style>

    <style name="BottomNavigationView.Inactive" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">@dimen/et_text_size_12</item>
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/color_828282</item>
    </style>

    <declare-styleable name="ExpandIconView">
        <attr name="eiv_roundedCorners" format="boolean" />
        <attr name="eiv_switchColor" format="boolean" />
        <attr name="eiv_color" format="color" />
        <attr name="eiv_colorMore" format="color" />
        <attr name="eiv_colorLess" format="color" />
        <attr name="eiv_colorIntermediate" format="color" />
        <attr name="eiv_animationDuration" format="integer" />
        <attr name="eiv_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="etv_max_line" format="integer" />
        <attr name="etv_background_resource" format="reference|color" />
    </declare-styleable>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/anim_slide_up</item>
        <item name="android:windowExitAnimation">@anim/anim_slide_down</item>
    </style>
</resources>